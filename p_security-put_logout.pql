PROCEDURE      P_SECURITY.PUT_LOGOUT:T  (157542,63963)  |Last amended Feb 13, 2014,17:46:03
retrieval UPDATE crwarn
ifthen ('<GUSER_ID>' NE '')
. ifthen ('<GSESSION_ID>' NE '')
.   record is SESSIONS via ('<GUSER_ID>' '<GSESSION_ID>')
.     LOGGED_OUT_DATE = today(0)
.     LOGGED_OUT_TIME = NOW(0)
.     LOGGED_OUT = 1
.   end rec is
. ELSE
.   write 'BLANK SESSION ID'
. endif
ELSE
. write 'BLANK USER_ID'
endif

end retrieval
END PROCEDURE
